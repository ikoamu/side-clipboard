{
  "name": "side-clipboard",
  "displayName": "side-clipboard",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Copy",
    "Clipboard",
    "Terminal"
  ],
  "pricing": "Free",
  "repository": {
    "type": "git",
    "url": "https://github.com/ikoamu/side-clipboard"
  },
  "icon": "images/icon.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sideClipboard.addItemFromClipboard",
        "category": "Side Clipboard",
        "title": "Add Item from Clipboard"
      }
    ],
    "views": {
      "side-clipboard-view": [
        {
          "type": "webview",
          "id": "sideClipboard.itemsView",
          "name": "Side Clipboard"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "side-clipboard-view",
          "title": "Side Clipboard",
          "icon": "images/codicon--clippy.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/codicons": "0.0.20",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "prettier": "3.3.3",
    "typescript": "^5.4.5"
  }
}
